ItemDisplay[MAG rin FILTLVL<2]:%NAME%%BLUE%

// # Show Identified Items #

// Show "Rerollable" tag on items that need it
ItemDisplay[ID MAG (cm3 ALVL>90)]:%NAME%{%TAN%Rerollable}
// So nothing we do after this applies to the stuff that was already ID'd
// For some reason, this applies to Common items as well, so we have to filter them out
// This rule is in a little bit of an awkward spot, since we need the ID'd crafting bases to still show the markers on them, but everything else shouldn't
// Don't include maps so that their descriptions work
ItemDisplay[ID !NMAG !(t11 OR t12 OR t13 OR t14 OR t15 OR t16 OR t17 OR t21 OR t22 OR t23 OR t24 OR t25 OR t26 OR t27 OR t28 OR t31 OR t32 OR t33 OR t34 OR t35 OR t36 OR t37 OR t38 OR t39 OR t3a)]:%NAME%{%NAME%}

// # Quivers #
// Curio - Magic Quivers can roll +3 after alvl 59
ItemDisplay[MAG (aqv OR cqv) ALVL>59 FILTLVL<2]:%NAME% %GRAY%[%WHITE%?%GRAY%]%BLUE%%BORDER-97%%DOT-1b%
// Otherwise only show in Normal
ItemDisplay[MAG (aqv OR cqv) DIFF=0]:%NAME%

// # Charms #
// Tier 1 Charms - 47 Small, 74 Medium, 91 Grand
ItemDisplay[MAG ((cm1 ALVL>46) OR (cm2 ALVL>73) OR (cm3 ALVL>90))]:%NAME% %GRAY%[%PURPLE%?%GRAY%]%BLUE%%BORDER-97%%DOT-9b%
// Tier 2 Charms - 33 Small, 34 Medium, 50 Grand
ItemDisplay[MAG ((cm1 ALVL>32) OR (cm2 ALVL>33) OR (cm3 ALVL>49))]:%NAME% %GRAY%[%RED%?%GRAY%]%BLUE%%BORDER-97%%DOT-0a%
// Default Charm display
ItemDisplay[MAG (cm1 OR cm2 OR cm3)]:%NAME% %GRAY%[%WHITE%?%GRAY%]%BLUE%%BORDER-97%%DOT-1f%

// # Jewels #
ItemDisplay[MAG jew]:%NAME% %GRAY%[%WHITE%?%GRAY%]%BLUE%%BORDER-97%%DOT-1f%

// # Magic Skill Items #
// Curio
// The only way to get +6. They can be strictly better than rares in a lot of cases.
// We need to only display this starting from ALvl 60 as well.
// Includes all H2H, all Daggers, all Staves, all Scepters, all Wands, all Orbs, all Necromancer Shields, all Clubs, 
// Sacred Targe, Sacred Rondache, all Druid Helmets, all Barbarian Helmets, all Gloves, all Circlets,
// Crystal Sword, Broad Sword, Flail, Maiden Javelin, Ceremonial Javelin, Matriarchal Javelin, Stag Bow,
// Ashwood Bow, Matriarchal Bow, Reflex Bow, Ceremonial Bow, Grand Matron Bow
// Strict - only show stuff that can be best in slot and can't be purchased (Gloves) - Circlets
ItemDisplay[MAG EQ7 ALVL>59]:%NAME% %GRAY%[%WHITE%?%GRAY%]%BLUE%%BORDER-97%%DOT-1b%
ItemDisplay[MAG FILTLVL<2 (CL5 OR WP4 OR WP11 OR WP13 OR WP12 OR CL6 OR CL4 OR clb OR spc OR 9cl OR 9sp OR 7cl OR 7sp OR pab OR pac OR CL2 OR CL1 OR EQ4 OR EQ7 OR crs OR bsd OR fla OR am5 OR ama OR amf OR am1 OR am6 OR amb OR am2 OR am7 OR amc) ALVL>59]:%NAME% %GRAY%[%WHITE%?%GRAY%]%BLUE%%BORDER-97%%DOT-1b%
// During levelling we still want to see all H2H, all Daggers, all Staves, all Scepters, all Wands, all Orbs, all Necromancer Shields, all Clubs, 
// all Druid Helmets, all Barbarian Helmets
ItemDisplay[MAG FILTLVL<2 (CL5 OR WP4 OR WP11 OR WP13 OR WP12 OR CL6 OR CL4 OR clb OR spc OR 9cl OR 9sp OR 7cl OR 7sp OR CL2 OR CL1)]:%NAME%%BORDER-97%%DOT-1b%

// ## Rare Items ##

// Weapons
// Tier 1 Weapons are shown as a part of a crafting section
// Tier 2 Weapons
// Decapitator, Silver Edged Axe, Ettin Axe, Tomahawk, Crusader Bow, Hydra Bow, Legend Spike, Devil Star
// Reinforced Mace, Cryptic Axe, Colossus Voulge, Ogre Axe, Thresher, Stygian Pike, Hyperion Spear
// Mancatcher, Colossal Sword, Balrog Blade, Highland Blade, Champion Sword, Falcata, Cryptic Sword
// Ataghan, Gorgon Crossbow, Pellet Bow, Stygian Pilum, Hyperion Javelin, Flying Knife
ItemDisplay[RARE FILTLVL<2 (7bt OR 7ba OR 72a OR 7ha OR 6l7 OR 6lw OR 7bl OR 7mt OR 7ma OR 7pa OR 7vo OR 7o7 OR 7s8 OR 7tr OR 7sr OR 7br OR 7fb OR 7gs OR 7cm OR 7b7 OR 7ss OR 7ls OR 7sm OR 6mx OR 6lx OR 7pi OR 7ja OR 7tk)]:%NAME% %GRAY%[%WHITE%?%GRAY%]%YELLOW%%BORDER-69%%DOT-1f%

// Shields
// Non-class Rare shields can roll +1 to class skills at alvl>64 and max PDR at alvl>84 (that means ilvl 92 btw)
// Bone Shield, Defender, Round Shield, Grim Shield, Heater, Troll Nest
ItemDisplay[RARE ALVL>84 (bsh OR xuc OR xml OR xsh OR uuc OR ush)]:%NAME% %GRAY%[%RED%?%GRAY%]%YELLOW%%BORDER-69%%DOT-0a%
ItemDisplay[RARE ALVL>64 FILTLVL<2 (bsh OR xuc OR xml OR xsh OR uuc OR ush)]:%NAME% %GRAY%[%WHITE%?%GRAY%]%YELLOW%%BORDER-69%%DOT-1f%

// Armor
// Rare Body Armor can roll +1 to class skills at alvl>64 and max PDR at alvl>84 (that means ilvl 92 btw)
// Breast Plate, Light Plate, Ghost Armor, Serpentskin Armor, Demonhide Armor, Trellished Armor, Cuirass, Mage Plate, Dusk Shroud, Wyrmhide, Scarab Husk, Wire Fleece, Archon Plate
ItemDisplay[RARE ALVL>84 (brs OR ltp OR xui OR xea OR xla OR xtu OR xrs OR xtp OR uui OR uea OR ula OR utu OR utp)]:%NAME% %GRAY%[%RED%?%GRAY%]%YELLOW%%BORDER-69%%DOT-0a%
ItemDisplay[RARE ALVL>64 FILTLVL<2 (brs OR ltp OR xui OR xea OR xla OR xtu OR xrs OR xtp OR uui OR uea OR ula OR utu OR utp)]:%NAME% %GRAY%[%WHITE%?%GRAY%]%YELLOW%%BORDER-69%%DOT-1f%

// Helm
// Rare Helmets can roll +1 to class skills at alvl>64
// Mask, Bone Helm, War Hat, Sallet, Casque, Death Mask, Grim Helm, Shako, Demonhead, Bone Visage, Hydraskull, Armet Van Buuren
ItemDisplay[RARE ALVL>64 FILTLVL<2 (msk OR bhm OR xap OR xkp OR xlm OR xsk OR xh9 OR uap OR usk OR uh9 OR ukp OR ulm)]:%NAME% %GRAY%[%WHITE%?%GRAY%]%YELLOW%%BORDER-69%%DOT-1f%
// Circlets with higher alvl than 89 are important, since they spawn +2 to classkills
ItemDisplay[EQ7 ALVL>89 RARE]:%NAME% %GRAY%[%PURPLE%?%GRAY%]%YELLOW%%BORDER-69%%DOT-9b%
// Default Circlets
ItemDisplay[EQ7 RARE]:%NAME% %GRAY%[%RED%?%GRAY%]%YELLOW%%BORDER-69%%DOT-0a%

// Gloves, Boots, Belts
// Tier1 - Gloves with ALVL>74 - All Affixes, Boots with ALVL>64 - Top FRW, Belts with ALVL>71 - All Affixes
ItemDisplay[RARE ((EQ4 ALVL>74) OR (EQ5 ALVL>64) OR (EQ6 ALVL>70))]:%NAME% %GRAY%[%PURPLE%?%GRAY%]%YELLOW%%BORDER-69%%DOT-9b%
// Tier2 - Gloves with ALVL>42 - Attack Speed, Boots with ALVL>36 - Top FRW, Belts with ALVL>49 - Max HP
ItemDisplay[RARE ((EQ4 ALVL>42) OR (EQ5 ALVL>36) OR (EQ6 ALVL>49))]:%NAME% %GRAY%[%RED%?%GRAY%]%YELLOW%%BORDER-69%%DOT-0a%
// The Rest
ItemDisplay[(EQ4 OR EQ5 OR EQ6) RARE]:%NAME% %GRAY%[%WHITE%?%GRAY%]%YELLOW%

// # Jewelry #
// Amulets with higher Alvl than 89 are important, since they spawn +2 to classkills
ItemDisplay[RARE amu ALVL>89]:%NAME% %GRAY%[%PURPLE%?%GRAY%]%YELLOW%%BORDER-69%%DOT-9b%
// Rings with higher Alvl than 85 are important, since they spawn all of the available mods
ItemDisplay[RARE rin ALVL>85]:%NAME% %GRAY%[%PURPLE%?%GRAY%]%YELLOW%%BORDER-69%%DOT-9b%
// Tier2 - Amulets with ALVL>35 - +1 to Class, Rings with ALVL>46 - Good Lifeleech
ItemDisplay[RARE ((amu ALVL>35) OR (rin ALVL>46))]:%NAME% %GRAY%[%RED%?%GRAY%]%YELLOW%%BORDER-69%%DOT-0a%
// Default rings and amulets
ItemDisplay[RARE (rin OR amu)]:%NAME% %GRAY%[%WHITE%?%GRAY%]%YELLOW%%BORDER-69%%DOT-1f%

// # Jewels #
ItemDisplay[jew RARE]:%NAME% %GRAY%[%RED%?%GRAY%]%YELLOW%%BORDER-69%%DOT-0a%

// # Quivers #
// Arrows and Bolts
ItemDisplay[(aqv OR cqv) RARE]:%NAME% %GRAY%[%WHITE%?%GRAY%]%YELLOW%%BORDER-69%%DOT-1f%%TIER-1%

// # Class-specific #
// The usual stuff - Druid Helms, Barbarian Helms, Necromancer Shields, Assassin Katars, Daggers, Sorceress Orbs, 
// Staves, Wands, Scepters, Clubs, Maiden Javelin, Ceremonial Javelin, Matriarchal Javelin
// None of them are strictly better than Magic items though, but all of them can be strictly better than a RW for a specific build
// Strict - only show Armors, since weapons are always worse than a +1 craft
ItemDisplay[RARE (CL1 OR CL2 OR CL4)]:%NAME% %GRAY%[%RED%?%GRAY%]%YELLOW%%BORDER-69%%DOT-0a%
ItemDisplay[RARE FILTLVL<2 (CL5 OR CL6 OR WP11 OR WP4 OR WP12 OR WP13 OR clb OR spc OR 9cl OR 9sp OR 7cl OR 7sp OR am5 OR ama OR amf)]:%NAME% %GRAY%[%RED%?%GRAY%]%YELLOW%%BORDER-69%%DOT-0a%

// ## Unique & Set Items ##

// # Memes #
// Includes 3 passenger seats
ItemDisplay[9st UNI]:%GOLD%Honda Sedan

// # Edgecases - Special Equipment#
ItemDisplay[rar OR rbe OR ram]:%NAME% %GRAY%[%PURPLE%?%GRAY%]%GOLD%%MAP%

// # Highlight rings & amulets #
// Hype it up
ItemDisplay[(rin OR amu) (UNI OR SET)]:%NAME% %GRAY%[%PURPLE%?%GRAY%]%GOLD%%MAP%

// # Highlight Elite Uniques&Sets #
// On average, they are supposed to be good. I don't want to put any more logic into unique stuff detection, because it'd take a toll on performance
ItemDisplay[(UNI OR SET) ELT]:%NAME% %GRAY%[%RED%?%GRAY%]%GOLD%%MAP%

// # Show all unique&set items #
// Duh
ItemDisplay[UNI OR SET]:%NAME%

// ## Whitelist ##
// # Leg rule #
// Wirt's Leg exception, since we actually want to see it, even if it has just 1 socket
ItemDisplay[leg]:%NAME%

// # Item Rarity Rules #
// Don't show Normal Items
ItemDisplay[(ARMOR OR WEAPON) NMAG SOCK=0]:
// Don't show Magic Items starting from Act 3
ItemDisplay[(ARMOR OR WEAPON) MAG ILVL>20]:
// Hide garbage 2+ Socket Staves, Wands, Necromancer Shields, since they will be shown during levelling
ItemDisplay[NMAG SOCK>1 (WP11 OR WP12 OR CL4)]:

// # Levelling Rules #
// Levelling rules are not dependant on the difficulty or the character level. Instead, they depend on the area level of a location the item is dropped in.
// The item level of a dropped item is never lower than the area level, therefore we can use it to filter out stuff.
// The basic idea here is simple: we want to filter low difficulty bases depending on the content the player is doing, 
// instead of CLVL (can be anything) or difficulty (while starting out in Hell you might still need some Nightmare bases, for instance)
// This alvl correlates with the alvl of Harem in Act 2 and last two levels of Hell Tower Cellar.
// Quite arbitrary, I know, but it represents around 85% of all of this difficulty's items being available, which means that, realistically, there's no reason to see items from the previous difficulty.
// Also, these rules don't conflict with any other rules that come before them - for example, you'll still see 5os Crystal Swords, even in high alvl areas.
ItemDisplay[NORM (ARMOR OR WEAPON) ILVL<47]:%NAME%
ItemDisplay[EXC (ARMOR OR WEAPON) ILVL<70]:%NAME%
ItemDisplay[ELT (ARMOR OR WEAPON) ILVL<82]:%NAME%
// The whitelist's backbone - hiding everything, including non-equipment equipment items - charms and arrows.
// Scary stuff - anything not mentioned in all of the previous rules will be hidden from sight.
ItemDisplay[ARMOR OR WEAPON OR rin OR amu OR jew OR cm1 OR cm2 OR cm3 OR aqv OR cqv]:

